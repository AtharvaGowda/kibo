  "files": [
    {
      "path": "registry/new-york/patterns/context-menu/canvas/context-menu-canvas-1.tsx",
      "content": "import {\n  ArrowDown,\n  ArrowDownToLine,\n  ArrowUp,\n  ArrowUpToLine,\n} from \"lucide-react\";\nimport {\n  ContextMenu,\n  ContextMenuContent,\n  ContextMenuItem,\n  ContextMenuSeparator,\n  ContextMenuShortcut,\n  ContextMenuTrigger,\n} from \"@/components/ui/context-menu\";\n\nexport const title = \"Layer Arrange Menu\";\n\nconst Example = () => (\n  <ContextMenu>\n    <ContextMenuTrigger className=\"flex h-[200px] w-full max-w-lg items-center justify-center rounded-md border border-dashed text-sm\">\n      Right click on element\n    </ContextMenuTrigger>\n    <ContextMenuContent className=\"w-56\">\n      <ContextMenuItem>\n        <ArrowUpToLine />\n        <span>Bring to Front</span>\n        <ContextMenuShortcut>⌘]</ContextMenuShortcut>\n      </ContextMenuItem>\n      <ContextMenuItem>\n        <ArrowUp />\n        <span>Bring Forward</span>\n        <ContextMenuShortcut>⌘⌥]</ContextMenuShortcut>\n      </ContextMenuItem>\n      <ContextMenuSeparator />\n      <ContextMenuItem>\n        <ArrowDown />\n        <span>Send Backward</span>\n        <ContextMenuShortcut>⌘⌥[</ContextMenuShortcut>\n      </ContextMenuItem>\n      <ContextMenuItem>\n        <ArrowDownToLine />\n        <span>Send to Back</span>\n        <ContextMenuShortcut>⌘[</ContextMenuShortcut>\n      </ContextMenuItem>\n    </ContextMenuContent>\n  </ContextMenu>\n);\n\nexport default Example;\n",
      "type": "registry:component"
    }
  ]
}