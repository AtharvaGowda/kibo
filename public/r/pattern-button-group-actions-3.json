      "content": "import {\n  ArchiveIcon,\n  CheckSquareIcon,\n  FolderIcon,\n  Trash2Icon,\n} from \"lucide-react\";\nimport { Button } from \"@/components/ui/button\";\nimport { ButtonGroup } from \"@/components/ui/button-group\";\n\nexport const title = \"Bulk Actions\";\n\nconst Example = () => (\n  <div className=\"flex flex-col gap-4\">\n    <ButtonGroup>\n      <Button size=\"sm\" variant=\"outline\">\n        <CheckSquareIcon />\n        Select All\n      </Button>\n      <Button size=\"sm\" variant=\"outline\">\n        <Trash2Icon />\n        Delete\n      </Button>\n      <Button size=\"sm\" variant=\"outline\">\n        <ArchiveIcon />\n        Archive\n      </Button>\n      <Button size=\"sm\" variant=\"outline\">\n        <FolderIcon />\n        Move\n      </Button>\n    </ButtonGroup>\n    <ButtonGroup>\n      <Button size=\"sm\" variant=\"outline\">\n        Select All (12)\n      </Button>\n      <Button size=\"sm\" variant=\"outline\">\n        Delete (3)\n      </Button>\n      <Button size=\"sm\" variant=\"outline\">\n        Archive (3)\n      </Button>\n    </ButtonGroup>\n  </div>\n);\n\nexport default Example;\n",
