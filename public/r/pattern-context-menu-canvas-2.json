  "title": "Context Menu Canvas 2",
  "description": "A context-menu pattern: context-menu-canvas-2",
  "registryDependencies": [],
  "files": [
    {
      "path": "registry/new-york/patterns/context-menu/canvas/context-menu-canvas-2.tsx",
      "content": "import {\n  AlignCenter,\n  AlignHorizontalJustifyCenter,\n  AlignLeft,\n  AlignRight,\n  AlignVerticalJustifyCenter,\n} from \"lucide-react\";\nimport {\n  ContextMenu,\n  ContextMenuContent,\n  ContextMenuItem,\n  ContextMenuSeparator,\n  ContextMenuSub,\n  ContextMenuSubContent,\n  ContextMenuSubTrigger,\n  ContextMenuTrigger,\n} from \"@/components/ui/context-menu\";\n\nexport const title = \"Align and Distribute Menu\";\n\nconst Example = () => (\n  <ContextMenu>\n    <ContextMenuTrigger className=\"flex h-[200px] w-full max-w-lg items-center justify-center rounded-md border border-dashed text-sm\">\n      Right click on selection\n    </ContextMenuTrigger>\n    <ContextMenuContent className=\"w-56\">\n      <ContextMenuSub>\n        <ContextMenuSubTrigger>\n          <div className=\"flex items-center gap-2\">\n            <AlignLeft />\n            <span>Align</span>\n          </div>\n        </ContextMenuSubTrigger>\n        <ContextMenuSubContent className=\"w-48\">\n          <ContextMenuItem>\n            <AlignLeft />\n            <span>Align Left</span>\n          </ContextMenuItem>\n          <ContextMenuItem>\n            <AlignCenter />\n            <span>Align Center</span>\n          </ContextMenuItem>\n          <ContextMenuItem>\n            <AlignRight />\n            <span>Align Right</span>\n          </ContextMenuItem>\n          <ContextMenuItem>\n            <AlignVerticalJustifyCenter />\n            <span>Align Top</span>\n          </ContextMenuItem>\n          <ContextMenuItem>\n            <AlignVerticalJustifyCenter />\n            <span>Align Middle</span>\n          </ContextMenuItem>\n          <ContextMenuItem>\n            <AlignVerticalJustifyCenter />\n            <span>Align Bottom</span>\n          </ContextMenuItem>\n        </ContextMenuSubContent>\n      </ContextMenuSub>\n      <ContextMenuSeparator />\n      <ContextMenuSub>\n        <ContextMenuSubTrigger>\n          <div className=\"flex items-center gap-2\">\n            <AlignHorizontalJustifyCenter />\n            <span>Distribute</span>\n          </div>\n        </ContextMenuSubTrigger>\n        <ContextMenuSubContent className=\"w-48\">\n          <ContextMenuItem>Distribute Horizontally</ContextMenuItem>\n          <ContextMenuItem>Distribute Vertically</ContextMenuItem>\n        </ContextMenuSubContent>\n      </ContextMenuSub>\n    </ContextMenuContent>\n  </ContextMenu>\n);\n\nexport default Example;\n",
      "type": "registry:component"
    }
  ]
}