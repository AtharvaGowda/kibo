      "path": "registry/new-york/patterns/dropdown-menu/profile/dropdown-menu-profile-4.tsx",
      "content": "\"use client\";\n\nimport { Check, ChevronsUpDown, Plus } from \"lucide-react\";\nimport { Avatar, AvatarFallback, AvatarImage } from \"@/components/ui/avatar\";\nimport { Button } from \"@/components/ui/button\";\nimport {\n  DropdownMenu,\n  DropdownMenuContent,\n  DropdownMenuItem,\n  DropdownMenuLabel,\n  DropdownMenuSeparator,\n  DropdownMenuTrigger,\n} from \"@/components/ui/dropdown-menu\";\n\nexport const title = \"Multi-Account Switcher\";\n\nconst Example = () => (\n  <DropdownMenu>\n    <DropdownMenuTrigger asChild>\n      <Button className=\"h-auto w-64 justify-between py-2\" variant=\"outline\">\n        <div className=\"flex items-center gap-2\">\n          <Avatar className=\"h-6 w-6\">\n            <AvatarImage\n              alt=\"@haydenbleasel\"\n              src=\"https://github.com/haydenbleasel.png\"\n            />\n            <AvatarFallback>HB</AvatarFallback>\n          </Avatar>\n          <div className=\"flex flex-col items-start\">\n            <span className=\"font-medium text-sm\">Personal Account</span>\n            <span className=\"text-muted-foreground text-xs\">\n              hello@haydenbleasel.com\n            </span>\n          </div>\n        </div>\n        <ChevronsUpDown className=\"h-4 w-4 shrink-0 opacity-50\" />\n      </Button>\n    </DropdownMenuTrigger>\n    <DropdownMenuContent align=\"start\" className=\"w-64\">\n      <DropdownMenuLabel>Accounts</DropdownMenuLabel>\n      <DropdownMenuSeparator />\n      <DropdownMenuItem className=\"flex items-center gap-2\">\n        <Avatar className=\"h-8 w-8\">\n          <AvatarImage\n            alt=\"@haydenbleasel\"\n            src=\"https://github.com/haydenbleasel.png\"\n          />\n          <AvatarFallback>HB</AvatarFallback>\n        </Avatar>\n        <div className=\"flex flex-col\">\n          <span className=\"font-medium text-sm\">Personal Account</span>\n          <span className=\"text-muted-foreground text-xs\">\n            hello@haydenbleasel.com\n          </span>\n        </div>\n        <Check className=\"ml-auto h-4 w-4\" />\n      </DropdownMenuItem>\n      <DropdownMenuItem className=\"flex items-center gap-2\">\n        <Avatar className=\"h-8 w-8\">\n          <AvatarFallback>AC</AvatarFallback>\n        </Avatar>\n        <div className=\"flex flex-col\">\n          <span className=\"font-medium text-sm\">Acme Corp</span>\n          <span className=\"text-muted-foreground text-xs\">team@acme.com</span>\n        </div>\n      </DropdownMenuItem>\n      <DropdownMenuItem className=\"flex items-center gap-2\">\n        <Avatar className=\"h-8 w-8\">\n          <AvatarFallback>ST</AvatarFallback>\n        </Avatar>\n        <div className=\"flex flex-col\">\n          <span className=\"font-medium text-sm\">Startup Inc</span>\n          <span className=\"text-muted-foreground text-xs\">\n            hello@startup.io\n          </span>\n        </div>\n      </DropdownMenuItem>\n      <DropdownMenuSeparator />\n      <DropdownMenuItem>\n        <Plus />\n        Add Account\n      </DropdownMenuItem>\n    </DropdownMenuContent>\n  </DropdownMenu>\n);\n\nexport default Example;\n",
      "type": "registry:component"
    }
  ]
