  "files": [
    {
      "path": "registry/new-york/patterns/command/dialog/command-dialog-6.tsx",
      "content": "\"use client\";\n\nimport { ArrowRight, Copy, Download, Share, Star, Trash } from \"lucide-react\";\nimport { useState } from \"react\";\nimport { Button } from \"@/components/ui/button\";\nimport {\n  CommandDialog,\n  CommandEmpty,\n  CommandGroup,\n  CommandInput,\n  CommandItem,\n  CommandList,\n  CommandShortcut,\n} from \"@/components/ui/command\";\n\nexport const title = \"Command Dialog Actions Menu\";\n\nconst Example = () => {\n  const [open, setOpen] = useState(false);\n\n  return (\n    <>\n      <Button onClick={() => setOpen(true)}>Quick Actions</Button>\n      <CommandDialog onOpenChange={setOpen} open={open}>\n        <CommandInput placeholder=\"Search actions...\" />\n        <CommandList>\n          <CommandEmpty>No actions found.</CommandEmpty>\n          <CommandGroup heading=\"Quick Actions\">\n            <CommandItem>\n              <ArrowRight />\n              <span>Open</span>\n              <CommandShortcut>↵</CommandShortcut>\n            </CommandItem>\n            <CommandItem>\n              <Copy />\n              <span>Copy</span>\n              <CommandShortcut>⌘C</CommandShortcut>\n            </CommandItem>\n            <CommandItem>\n              <Star />\n              <span>Add to Favorites</span>\n              <CommandShortcut>⌘D</CommandShortcut>\n            </CommandItem>\n          </CommandGroup>\n          <CommandGroup heading=\"File Actions\">\n            <CommandItem>\n              <Download />\n              <span>Download</span>\n              <CommandShortcut>⌘⇧D</CommandShortcut>\n            </CommandItem>\n            <CommandItem>\n              <Share />\n              <span>Share</span>\n              <CommandShortcut>⌘⇧S</CommandShortcut>\n            </CommandItem>\n            <CommandItem>\n              <Trash />\n              <span>Delete</span>\n              <CommandShortcut>⌘⌫</CommandShortcut>\n            </CommandItem>\n          </CommandGroup>\n        </CommandList>\n      </CommandDialog>\n    </>\n  );\n};\n\nexport default Example;\n",
      "type": "registry:component"
    }
  ]
}