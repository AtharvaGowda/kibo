{
  "$schema": "https://ui.shadcn.com/schema/registry-item.json",
  "name": "pattern-form-multi-field-7",
  "type": "registry:component",
  "title": "Form Multi Field 7",
  "description": "A form pattern: form-multi-field-7",
  "registryDependencies": [],
  "files": [
    {
      "path": "registry/new-york/patterns/form/multi-field/form-multi-field-7.tsx",
      "content": "\"use client\";\n\nimport { zodResolver } from \"@hookform/resolvers/zod\";\nimport { useForm } from \"react-hook-form\";\nimport { z } from \"zod\";\nimport { Button } from \"@/components/ui/button\";\nimport {\n  Form,\n  FormControl,\n  FormField,\n  FormItem,\n  FormLabel,\n  FormMessage,\n} from \"@/components/ui/form\";\nimport { Input } from \"@/components/ui/input\";\n\nexport const title = \"Long Form with Many Fields\";\n\nconst formSchema = z.object({\n  username: z.string().min(3),\n  email: z.string().email(),\n  firstName: z.string().min(2),\n  lastName: z.string().min(2),\n  phone: z.string().regex(/^\\+?[1-9]\\d{1,14}$/),\n  street: z.string().min(2),\n  city: z.string().min(2),\n  state: z.string().min(2),\n  zipCode: z.string().regex(/^\\d{5}(-\\d{4})?$/),\n});\n\nconst Example = () => {\n  const form = useForm<z.infer<typeof formSchema>>({\n    resolver: zodResolver(formSchema),\n    defaultValues: {\n      username: \"\",\n      email: \"\",\n      firstName: \"\",\n      lastName: \"\",\n      phone: \"\",\n      street: \"\",\n      city: \"\",\n      state: \"\",\n      zipCode: \"\",\n    },\n  });\n\n  function onSubmit(values: z.infer<typeof formSchema>) {\n    console.log(values);\n  }\n\n  return (\n    <div className=\"w-full max-w-md\">\n      <Form {...form}>\n        <form className=\"space-y-6\" onSubmit={form.handleSubmit(onSubmit)}>\n          <div className=\"space-y-4\">\n            <h3 className=\"font-medium text-lg\">Account Details</h3>\n            <FormField\n              control={form.control}\n              name=\"username\"\n              render={({ field }) => (\n                <FormItem>\n                  <FormLabel>Username</FormLabel>\n                  <FormControl>\n                    <Input\n                      className=\"bg-background\"\n                      placeholder=\"johndoe\"\n                      {...field}\n                    />\n                  </FormControl>\n                  <FormMessage />\n                </FormItem>\n              )}\n            />\n            <FormField\n              control={form.control}\n              name=\"email\"\n              render={({ field }) => (\n                <FormItem>\n                  <FormLabel>Email</FormLabel>\n                  <FormControl>\n                    <Input\n                      className=\"bg-background\"\n                      placeholder=\"john@example.com\"\n                      type=\"email\"\n                      {...field}\n                    />\n                  </FormControl>\n                  <FormMessage />\n                </FormItem>\n              )}\n            />\n          </div>\n          <div className=\"space-y-4\">\n            <h3 className=\"font-medium text-lg\">Personal Information</h3>\n            <div className=\"grid grid-cols-2 gap-4\">\n              <FormField\n                control={form.control}\n                name=\"firstName\"\n                render={({ field }) => (\n                  <FormItem>\n                    <FormLabel>First Name</FormLabel>\n                    <FormControl>\n                      <Input\n                        className=\"bg-background\"\n                        placeholder=\"John\"\n                        {...field}\n                      />\n                    </FormControl>\n                    <FormMessage />\n                  </FormItem>\n                )}\n              />\n              <FormField\n                control={form.control}\n                name=\"lastName\"\n                render={({ field }) => (\n                  <FormItem>\n                    <FormLabel>Last Name</FormLabel>\n                    <FormControl>\n                      <Input\n                        className=\"bg-background\"\n                        placeholder=\"Doe\"\n                        {...field}\n                      />\n                    </FormControl>\n                    <FormMessage />\n                  </FormItem>\n                )}\n              />\n            </div>\n            <FormField\n              control={form.control}\n              name=\"phone\"\n              render={({ field }) => (\n                <FormItem>\n                  <FormLabel>Phone</FormLabel>\n                  <FormControl>\n                    <Input\n                      className=\"bg-background\"\n                      placeholder=\"+1 (555) 123-4567\"\n                      type=\"tel\"\n                      {...field}\n                    />\n                  </FormControl>\n                  <FormMessage />\n                </FormItem>\n              )}\n            />\n          </div>\n          <div className=\"space-y-4\">\n            <h3 className=\"font-medium text-lg\">Address</h3>\n            <FormField\n              control={form.control}\n              name=\"street\"\n              render={({ field }) => (\n                <FormItem>\n                  <FormLabel>Street</FormLabel>\n                  <FormControl>\n                    <Input\n                      className=\"bg-background\"\n                      placeholder=\"123 Main St\"\n                      {...field}\n                    />\n                  </FormControl>\n                  <FormMessage />\n                </FormItem>\n              )}\n            />\n            <div className=\"grid grid-cols-3 gap-4\">\n              <FormField\n                control={form.control}\n                name=\"city\"\n                render={({ field }) => (\n                  <FormItem>\n                    <FormLabel>City</FormLabel>\n                    <FormControl>\n                      <Input\n                        className=\"bg-background\"\n                        placeholder=\"New York\"\n                        {...field}\n                      />\n                    </FormControl>\n                    <FormMessage />\n                  </FormItem>\n                )}\n              />\n              <FormField\n                control={form.control}\n                name=\"state\"\n                render={({ field }) => (\n                  <FormItem>\n                    <FormLabel>State</FormLabel>\n                    <FormControl>\n                      <Input\n                        className=\"bg-background\"\n                        placeholder=\"NY\"\n                        {...field}\n                      />\n                    </FormControl>\n                    <FormMessage />\n                  </FormItem>\n                )}\n              />\n              <FormField\n                control={form.control}\n                name=\"zipCode\"\n                render={({ field }) => (\n                  <FormItem>\n                    <FormLabel>ZIP</FormLabel>\n                    <FormControl>\n                      <Input\n                        className=\"bg-background\"\n                        placeholder=\"10001\"\n                        {...field}\n                      />\n                    </FormControl>\n                    <FormMessage />\n                  </FormItem>\n                )}\n              />\n            </div>\n          </div>\n          <Button className=\"w-full\" type=\"submit\">\n            Complete Registration\n          </Button>\n        </form>\n      </Form>\n    </div>\n  );\n};\n\nexport default Example;\n",
      "type": "registry:component"
    }
  ]
}