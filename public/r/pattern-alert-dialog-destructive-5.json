{
  "$schema": "https://ui.shadcn.com/schema/registry-item.json",
  "name": "pattern-alert-dialog-destructive-5",
  "type": "registry:component",
  "title": "Alert Dialog Destructive 5",
  "description": "A alert-dialog pattern: alert-dialog-destructive-5",
  "registryDependencies": [],
  "files": [
    {
      "path": "registry/new-york/patterns/alert-dialog/destructive/alert-dialog-destructive-5.tsx",
      "content": "import { X } from \"lucide-react\";\nimport {\n  AlertDialog,\n  AlertDialogAction,\n  AlertDialogCancel,\n  AlertDialogContent,\n  AlertDialogDescription,\n  AlertDialogFooter,\n  AlertDialogHeader,\n  AlertDialogTitle,\n  AlertDialogTrigger,\n} from \"@/components/ui/alert-dialog\";\nimport { Button } from \"@/components/ui/button\";\n\nexport const title = \"Destructive with Custom Styled Buttons\";\n\nconst Example = () => (\n  <AlertDialog>\n    <AlertDialogTrigger asChild>\n      <Button variant=\"destructive\">Remove Member</Button>\n    </AlertDialogTrigger>\n    <AlertDialogContent>\n      <AlertDialogHeader>\n        <div className=\"flex items-start justify-between\">\n          <div className=\"flex items-center gap-2\">\n            <X className=\"size-5 text-destructive\" />\n            <AlertDialogTitle>Remove Team Member</AlertDialogTitle>\n          </div>\n        </div>\n        <AlertDialogDescription>\n          Are you sure you want to remove this member from the team? They will\n          lose access to all projects and data.\n        </AlertDialogDescription>\n      </AlertDialogHeader>\n      <AlertDialogFooter>\n        <AlertDialogCancel>Keep Member</AlertDialogCancel>\n        <AlertDialogAction className=\"bg-destructive text-white hover:bg-destructive/90\">\n          <X className=\"size-4\" />\n          Remove Member\n        </AlertDialogAction>\n      </AlertDialogFooter>\n    </AlertDialogContent>\n  </AlertDialog>\n);\n\nexport default Example;\n",
      "type": "registry:component"
    }
  ]
}